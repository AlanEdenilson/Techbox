<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <title>Herramientas</title>
</head>
<body>
    <table class="table table-light">
        <thead class="thead-light">
            <tr>
                <th>id_herramientas</th>
                <th>Nomnbre</th>
                <th>Estado</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
        
            <% for(let herramientas = 0; herramientas < herra.length; herramientas++){%>
            <tr>
                <td> <%= herra[herramientas].id_herramientas%> </td>
                <td> <%= herra[herramientas].Nombre%> </td>
                <td> <%= herra[herramientas].Estado%> </td>
                <td>Editar | Borrar </td>
            </tr>
                <% } %>

            <tr>
                <td> - </td>
                <td> - </td>
                <td> - </td>
                <td> - </td>
                <td>Editar | Borrar </td>
            </tr>
        </tbody>
    </table>

</body>
</html>